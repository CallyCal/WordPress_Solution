{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westeurope"
        },
        "storageAccountName": {
            "value": "wordpressgrs01"
        },
        "accountType": {
            "value": "Standard_GRS"
        },
        "kind": {
            "value": "StorageV2"
        },
        "accessTier": {
            "value": "Hot"
        },
        "minimumTlsVersion": {
            "value": "TLS1_0"
        },
        "supportsHttpsTrafficOnly": {
            "value": true
        },
        "allowBlobPublicAccess": {
            "value": true
        },
        "networkAclsBypass": {
            "value": "AzureServices"
        },
        "networkAclsDefaultAction": {
            "value": "Allow"
        },
        "isBlobSoftDeleteEnabled": {
            "value": true
        },
        "blobSoftDeleteRetentionDays": {
            "value": 30
        },
        "isVersioningEnabled": {
            "value": true
        },
        "isShareSoftDeleteEnabled": {
            "value": true
        },
        "shareSoftDeleteRetentionDays": {
            "value": 30
        }
    }
}